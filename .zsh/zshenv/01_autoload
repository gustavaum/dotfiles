# autoload executables in $fpath
for dirname in $fpath; do
    case "$dirname" in
        $ZDOTDIR/.zsh*) fns=( $dirname/*~*~(-N.x:t) ) ;;
        *) fns=( $dirname/*~*~(-N.:t)  ) ;;
    esac

    (( $#fns )) && autoload "$fns[@]"
done

# make colors available
autoload -U colors ;  colors

# vim:ft=zsh


