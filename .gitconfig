[user]
    email = mattrogers@sbcglobal.net
    name = Matt Rogers

[url "git://anongit.kde.org/"]
    insteadOf = kde:
[url "git@git.kde.org:"]
    pushInsteadOf = kde:

[url "https://github.com/"]
    insteadOf = github:
[url "git@github.com:"]
    pushInsteadOf = github:

[url "git@github.com:mattr-/"]
    insteadOf = mattr:
    pushInsteadOf = mattr:

[url "git@github.com:TracksApp/"]
    insteadOf = tracks:
    pushInsteadOf = tracks:

[url "git@github.com:dallasrb/"]
    insteadOf = dallasrb:
    pushInsteadOf = dallasrb:

[url "git@github.com:Hireology/"]
    insteadOf = hireology:
    pushInsteadOf = hireology:



[diff]
[color]
    ui = auto

[alias]
    aa = add --all
    di = diff 
    dic = diff --cached
    head = !git l -1
    st = status -sb
    stuno = status -uno
    co = checkout
    ci = commit
    cane = commit --amend --no-edit
    ff = merge --ff-only
    pff = pull --ff-only 
    last = !git l -1
    last10 = !git l -10
    l = "!source ~/.githelpers && pretty_git_log"
    la = !git l --all
    sm = submodule
    bd = branch -d
    b = branch
    ba = branch -a
    dc = diff --cached
    undo = "!source ~/.githelpers && undo_local_changes"
    sdcp = "!source ~/.githelpers && dryrun_dcommit_with_patch"
    sdc = svn dcommit
    t = tag
    td = tag -d
    count = shortlog -sn
    credit = "!source ~/.githelpers && change_author"
    rv = remote -v
    pullr = pull --rebase
    ssp = stash show -p
    sl = stash list
    sa = stash apply
    remove-merged = "!source ~/.githelpers && remove_merged_branches"

[core]
    excludesfile = /opt/boxen/config/git/gitignore
[hub]
    protocol = https
[push]
    default = current
[credential]
helper = /opt/boxen/bin/boxen-git-credential
[github]
	user = mattr-
